{
  "name": "timeline",
  "version": "1.0.0",
  "description": "A comprehensive Node.js application that processes image geolocation using Google Maps timeline data and temporal interpolation",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node tests/test-runner.js",
    "test:gps": "node tests/test-runner.js gps-extraction",
    "test:write": "node tests/test-runner.js gps-writing",
    "test:coords": "node tests/test-runner.js coordinates",
    "test:watch": "node --watch tests/test-runner.js"
  },
  "keywords": [
    "image",
    "geolocation",
    "exif",
    "gps",
    "timeline",
    "interpolation",
    "maps"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "piexifjs": "^1.0.6",
    "readline-sync": "^1.4.10",
    "sharp": "^0.33.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
